{
  "name": "poe-editor-proxy",
  "version": "1.4.1",
  "description": "Cloudflare Worker for dynamic Open Graph previews and root-level routing",
  "main": "dist/worker.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --config wrangler.toml --env dev",
    "dev:remote": "wrangler dev --remote --config wrangler.toml --env dev",
    "deploy": "wrangler deploy",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "react": "^19.2.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260219.0",
    "@eslint/js": "^9.39.2",
    "@types/react": "^19.2.14",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "miniflare": "^4.20260219.0",
    "prettier": "^3.8.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18",
    "wrangler": "^4.67.0"
  }
}
